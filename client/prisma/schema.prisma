generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Client {
  id        String   @id @default(uuid())
  creatdAt  DateTime @default(now())
  updatedAt DateTime @updatedAt
}

model ClientSecret {
  id        String   @id @default(uuid())
  secret    String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

model ClientApplication {
  id        String   @id @default(uuid())
  name      String
  clientId  String
  callback  String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
